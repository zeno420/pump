<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="720" prefWidth="1080"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.DayController">
    <children>
        <Label text="Name:" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0"/>
        <TextField fx:id="dayNameField" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="20.0"/>
        <Label text="Workouts:" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="50.0"/>
        <ListView fx:id="programWorkoutsListView" prefHeight="163.0" prefWidth="200.0" AnchorPane.bottomAnchor="60.0"
                  AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="140.0"/>
        <Button fx:id="addWorkoutButton" mnemonicParsing="false" onAction="#addWorkoutToDay"
                text="add" AnchorPane.leftAnchor="350.0" AnchorPane.topAnchor="50.0"/>
        <ComboBox fx:id="workoutComboBox" prefHeight="26.0" prefWidth="171.0" AnchorPane.leftAnchor="150.0"
                  AnchorPane.topAnchor="50.0"/>
        <Button fx:id="removeWorkoutButton" mnemonicParsing="false" onAction="#removeWorkoutFromDay"
                text="remove selected Workout" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0"/>
        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="cancel"
                AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="115.0"/>
        <Button fx:id="saveDayButton" mnemonicParsing="false" onAction="#saveDay" text="save"
                AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0"/>
    </children>
</AnchorPane>