<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.WorkoutController">
    <children>
        <ListView fx:id="satzListView" layoutX="200.0" layoutY="94.0" prefHeight="200.0" prefWidth="200.0"/>
        <Button fx:id="previousBtn" layoutX="200.0" layoutY="347.0" mnemonicParsing="false" onAction="#previousUebung"
                text="&lt;&lt;"/>
        <Button fx:id="nextBtn" layoutX="360.0" layoutY="347.0" mnemonicParsing="false" onAction="#nextUebung"
                text="&gt;&gt;"/>
        <Label fx:id="uebungNameLabel" layoutX="282.0" layoutY="60.0" text="Label"/>
        <Label fx:id="indexLabel" layoutX="282.0" layoutY="26.0" text="Label"/>
    </children>
</AnchorPane>
